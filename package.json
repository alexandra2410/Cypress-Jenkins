{
  "name": "cypress-e2e",
  "version": "1.0.0",
  "description": "e2e framework testing cypress",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cy:open": "cypress open",
    "merge-reports": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json",
    "generate-report": "marge cypress/reports/merged-report.json -f report -o cypress/reports/html",
    "cy:runDefaul": "npx cypress run --spec 'cypress/e2e/Retry-Ability.cy.js' --headed ",
    "cy:runFirefox": "npx cypress run --spec 'cypres/e2e/pom/*cy.js' --headed --browser firefox",
    "cy:runFirefoxHeadless": "npx cypress run --spec 'cypress/e2e/pom/*.cy.js' --headless --browser firefox",
    "cy:runChrome": "npx cypress run --spec 'cypres/e2e/pom/*cy.js' --headed --browser chrome",
    "cy:browserstack": "browserstack-cypress run --sync --spec cypress/e2e/homeSauce.cy.js"
  },
  "keywords": [
    "e2e",
    "automation",
    "framework",
    "cypress"
  ],
  "author": "QAlaub",
  "license": "ISC",
  "devDependencies": {
   "cypress": "^13.15.0",
   "@cypress/grep": "^4.1.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-xpath": "^2.0.1"
  }
}
